{
"compilerOptions": {
    "module": "es2020",
    "strictNullChecks": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES2020",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "declaration": true,
    "moduleResolution": "node",
    "types": ["node"]
}
}