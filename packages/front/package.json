{
  "name": "@lp/front",
  "version": "1.2.0",
  "description": "frontend",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --env mode=development",
    "codegen": "graphql-codegen --config codegen.ts",
    "prod": "webpack --env mode=production",
    "cypress": "npx cypress open",
    "cypress:run": "npx cypress run --browser chrome",
    "stylelint": "stylelint src",
    "lint": "eslint src",
    "ready": "yarn codegen && yarn eslint --fix && yarn stylelint --fix",
    "serve": "http-server",
    "clean": "rimraf dist src/generated *.tsbuildinfo public node_modules",
    "ts": "tsc -b .",
    "codegen:watch": "graphql-codegen --config codegen.ts -w",
    "start": "yarn dev",
    "analyze": "yarn prod --env stats=server"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@lp/ui": "^1.3.0",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv-webpack": "^8.0.1",
    "graphql": "^16.8.1",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.0.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0"
  },
  "devDependencies": {
    "cypress": "^13.17.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "http-server": "^14.1.0",
    "mongoose": "^8.0.3",
    "webpack-dev-server": "^4.9.0"
  }
}
