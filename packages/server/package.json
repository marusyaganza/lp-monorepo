{
  "name": "graph-sp",
  "version": "0.0.0",
  "description": "app for Spanish learners",
  "main": "app.js",
  "scripts": {
    "start": "nodemon src/app.js --ignore 'src/db/db.json'",
    "build": "node src/app.js",
    "ts": "tsc -b .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon --watch './**/*.{ts,js}' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/app.ts",
    "dev": "tsc-watch --onSuccess \"npm run watch\""
  },
  "author": "Marusya Ganza",
  "license": "MIT",
  "dependencies": {
    "apollo-server": "^3.7.0",
    "date-fns": "^2.5.0",
    "graphql": "^16.5.0",
    "jsonwebtoken": "^8.5.1",
    "lowdb": "^1.0.0",
    "nanoid": "^2.1.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
